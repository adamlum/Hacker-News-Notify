<% form_for @user_session do |f| %>
  <table>
    <tr>
      <td style="text-align:right"><%= f.label :username %></td>
      <td><%= f.text_field :username %></td>
    </tr>
    <tr>
      <td style="text-align:right"><%= f.label :password %></td>
      <td><%= f.password_field :password %></td>
    </tr>
    <tr><td colspan="2" style="text-align:right"><%= f.submit "Submit" %></td></tr>
  </table>
<% end %>
